{
	"name": "Dupbit Extension",
	"description": "Interact with Dupbit",
	"icons": {
		"128": "images/icon.png"
	},
	"version": "2.0.0",
	"manifest_version": 2,
	"background": {
		"scripts": [ "jquery.js", "background.js", "src/facebook.js" ]
	},
	"browser_action": {
		"default_icon": "images/icon.png",
		"default_title": "Dupbit Extension",
		"default_popup": "popup.html"
	},
	"permissions": [ "downloads","activeTab","webRequest","https://dupbit.com/" ]
}
